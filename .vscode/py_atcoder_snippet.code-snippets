            "        if nx > -1 and nx < W and ny > -1 and ny < H:",
            "            if islands[ny][nx] == \".\": #海の場合",
            "                sea += 1",
            "            elif islands[ny][nx] == '#': #陸地の場合",
            "                land += 1",
            "                dfs(nx, ny)",
        ],
        "description": "地続きの島とその周りの海に面した辺をカウント"
